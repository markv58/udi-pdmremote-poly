<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- Wemo Control Node -->
    <nodeDef id="PDMREMOTE_CTRL" nls="CTRL">
        <editors />
        <sts>
            <st id="ST" editor="BOOL" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DISCOVER" />
            </accepts>
        </cmds>
    </nodeDef>
    <!-- Wemo Switch Node -->
    <nodeDef id="HB6SPEED" nls="HB6SPEED">
        <editors />
        <sts>
            <st id="ST" editor="SPEED" />
            <st id="GV0" editor="MODE" />
            <st id="GV1" editor="GPIO" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
                <cmd id="DFON" />
                <cmd id="DFOF" />
                <cmd id="BRT" />
                <cmd id="DIM" />
                <cmd id="QUERY" />
                <cmd id="SET_MODE" editor="MODE">
                <cmd id="SET_SPEED" editor="SPEED">
                <cmd id="SET_GPIO" editor="GPIO">
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
